<div class="container">

  <div class="col-md-12">
    <div id="greeting" class="col-md-12"><h1>Welcome!</h1></div>
    <div id="logo" class="col-md-12"></div>
    <div class="col-md-12 login1">
    <h3 class="login">Already a member?</h3>
    <button class="btn btn-default btn-lg">Sign in</button>
  </div> <!-- close col-md-12 -->
    <div class="col-md-12 login1">
      <h4 class="login">-----------------Or---------------</h4>
    <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
          <%= link_to "Sign up", new_registration_path(resource_name), class: "btn btn-default btn-lg signup" %><br />
          <% end -%>
    </div> <!-- close col-md-12 -->
  </div> <!-- close col-xs-6 div -->

 
      <div class="signin">

          <div class="fillout">
              
            
              <div class="form-inline"><%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

                <div class="form-group">

                <%= f.label :email, :class =>"sr-only"%>
                <%= f.email_field :email, autofocus: true, :class=>"form-control input-lg", placeholder:"Email address"%>
                </div> <!-- close form-group div -->

                 <div class="form-group">
                <%= f.label :password, :class=>"sr-only"%>
                <%= f.password_field :password, autocomplete: "off", :class=>"form-control input-lg", placeholder:"password" %>
                </div> <!-- close form-group div -->
               
               <%= f.submit "Sign in", :class=>"btn btn-primary btn-lg" %><br>


                 <div class="checkbox">
                  <% if devise_mapping.rememberable? -%>
                    <%= f.check_box :remember_me %>
                    <%= f.label :remember_me, :class =>"login" %>
                  <% end -%>
                </div> <!-- close checkbox -->
    

            <% end %>
   
       </div> <!-- close fillout div -->
       </div> <!-- close form-inline -->

      <%= render "users/shared/links" %>
        </div> <!-- close signin div -->

  </div> <!-- close col-xs-6 div -->

</div><!--  close container div -->



